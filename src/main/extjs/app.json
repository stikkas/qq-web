{
	"name": "qqext",
	"indexHtmlPath": "index.html",
	"classpath": "${app.dir}/app",
	"overrides": "${app.dir}/overrides",
	"framework": "ext",
	"theme": "ext-theme-classic",
	"requires": [
		"ext-locale"
	],
	"locale": "ru",
	"sass": {
		"namespace": "qqext"
	},
	"url": null,
	"js": [
		{
			"path": "${ext.dir}/build/ext-all-debug.js"
		},
		{
			"path": "app.js",
			"bundle": true
		}
	],
	"css": [
		{
			"path": "resources/css/styles.css",
			"bootstrap": false
		},
		{
			"path": "resources/css/icons.css",
			"bootstrap": false
		},
		{
			"path": "bootstrap.css",
			"bootstrap": true
		}
	],
	"production": {
	},
	"testing": {
	},
	"development": {
	},
	"bootstrap": {
		"base": "${app.dir}",
		"manifest": "bootstrap.json",
		"microloader": "bootstrap.js",
		"css": "bootstrap.css"
	},
	"output": {
		"base": "../webapp/"
	},
	"appCache": {
		"cache": [
			"index.html"
		],
		"network": [
			"*"
		],
		"fallback": []
	},
	"resources": [
		"resources/images"
	],
	"ignore": [
		"(^|/)CVS(/?$|/.*?$)"
	],
	"archivePath": "archive",
	"slicer": {
		"js": [
			{
				"path": "${app.dir}/sass/example/custom.js",
				"isWidgetManifest": true
			}
		]
	},
	"id": "1baf9dca-2489-47cd-a9d5-b908aea41b3a"
}
